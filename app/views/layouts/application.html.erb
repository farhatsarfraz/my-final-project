<!DOCTYPE html>
<html>
<head>
  <title>GoogleMap</title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  
  <script src="//maps.google.com/maps/api/js?key=AIzaSyAAglFai7NmSBkxvY2p2Vw61IhzxL3UO7Q"></script>  
  <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>    
  <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>
  <script src='//underscorejs.org/underscore-min.js' type='text/javascript'></script>

</head>

<body>

<%= render 'layouts/navigation' %>

<div class="container">
  <% if notice %><div class="alert alert-info"><%= notice %><% end %>
  <% if alert %><div class="alert alert-warning"><%= alert %><% end %>

  <%= yield %>
</div>
</body>
</html>
